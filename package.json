{
  "name": "analytics-report-api",
  "version": "1.0.0",
  "description": "An API for service data generated by Analytics Reporter",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon src/index.js",
    "postinstall": "npm run migrate",
    "pretest": "NODE_ENV=test npm run migrate",
    "test": "NODE_ENV=test `npm bin`/mocha test/**/*.test.js --exit",
    "migrate": "`npm bin`/knex migrate:latest",
    "lint": "`npm bin`/eslint src/**.js test/**.js *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/analytics-reporter-api.git"
  },
  "author": "Jonathan Hooper",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/18F/analytics-reporter-api/issues"
  },
  "homepage": "https://github.com/18F/analytics-reporter-api#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.20.0",
    "extend": ">= 3.0.2",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "proxyquire": "^2.1.3",
    "supertest": "^4.0.2",
    "supertest-as-promised": "^4.0.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-winston": "^4.0.3",
    "knex": "^0.21.1",
    "lodash": ">= 4.17.13",
    "pg": "^8.3.2",
    "sinon": "^9.0.2",
    "winston": "^3.0.0"
  },
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  }
